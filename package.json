{
  "name": "typescript_boiler",
  "version": "1.0.1",
  "description": "A simple webpack boilerplate for front end projects using Typescript/Sass",
  "keywords": [
    "typescript",
    "sass",
    "webpack"
  ],
  "main": "index.js",
  "scripts": {
    "test": "karma start karma.conf.js",
    "tslint": "tslint",
    "stylelint": "stylelint",
    "server": "webpack-dev-server -d --devtool --inline  --content-base --client-log-level warning --env.dev",
    "start": "npm run server",
    "build": "webpack --progress --colors --display-modules",
    "tsd": "tsd"
  },
  "author": "Ryan Ruan",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "awesome-typescript-loader": "^2.2.4",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.1.2",
    "node-sass": "^3.10.1",
    "phantomjs": "^2.1.7",
    "postcss-loader": "^1.0.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "stylelint-config-standard": "^14.0.0",
    "stylelint-webpack-plugin": "^0.4.0",
    "ts-loader": "^0.9.5",
    "tsd": "^0.6.5",
    "tslint": "^3.15.1",
    "tslint-eslint-rules": "^2.1.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.6",
    "typings": "^1.5.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "lodash": "^4.16.6",    
    "normalize-scss": "^6.0.0",
    "pixi.js": "^4.1.1"
  }
}
